@model IEnumerable<ToTheMoon.Models.Space>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.capacity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.used)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.increase)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PIKey)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.capacity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.used)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.increase)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PIKey)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.key }) |
            @Html.ActionLink("Details", "Details", new { id=item.key }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.key })
        </td>
    </tr>
}

</table>
