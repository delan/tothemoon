@model IEnumerable<ToTheMoon.Models.Request>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Space")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HumanReadableRequestString)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RequestTimestamp)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HumanReadableRequestString)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RequestTimestamp)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.RequestID }) |
            @Html.ActionLink("Details", "Details", new { id=item.RequestID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.RequestID })
        </td>
    </tr>
}

</table>
